var msg='enter the percentageof student:';
var a=prompt(msg);
if(a>85)
document.write('Grade-A');}
else if (a>85 && a<=80)
document.write('Grade-B-');}
else if (a>70 && a<=80)
document.write('Grade-c');}
else if (a>60 && a<=70)
document.write('Grade D-');}
else if (a>60 && a<=40)
document.write('Grade F-');}
else if a>40 && a<=35)